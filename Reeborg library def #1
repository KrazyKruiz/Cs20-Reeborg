
#3 lefts make a right
def turn_right():
    turn_left()
    turn_left()
    turn_left()
    
#double move and take for ease    
def move2andtake():
    move()
    take()
    move()
    take()
    
#double move for ease
def move2():
    move()
    move()
    
#180 degree turn
def halfturn():
    turn_right()
    turn_right()
    
#put 2 times
def put2():
    put()
    put()
    
    
